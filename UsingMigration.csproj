	</ItemGroup>
	
	<ItemGroup>
	  <PackageReference Include="Evolve" Version="3.2.0" />
	  <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="8.0.0" />
	  <PackageReference Include="Npgsql" Version="8.0.1" />
	</ItemGroup>

</Project>
